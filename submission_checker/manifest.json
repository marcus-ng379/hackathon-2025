{
  "manifest_version": 3,
  "name": "LeetCode Submission Checker",
  "version": "1.0",
  "description": "Checks if a LeetCode submission was made and whether it was accepted.",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/*"],
      "js": ["content.js"]
    }
  ],
  "host_permissions": [
    "https://leetcode.com/*"
  ]
}
