{
  "manifest_version": 3,
  "name": "LeetCode Submission Checker",
  "version": "1.0",
  "description": "Checks if a LeetCode problem has been submitted successfully.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://leetcode.com/problems/*"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["https://leetcode.com/*"]
}
